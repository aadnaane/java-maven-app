pipeline {
  agent any
  stages {
    stage("build") {
      step {
        echo ("building the app")
      }
    }
    tage("test") {
      step {
        echo ("testing the app")
      }
    }
    tage("deploy") {
      step {
        echo ("deploying the app")
      }
    }
    tage("publish") {
      step {
        echo ("publishing the app")
      }
    }
  }
}
