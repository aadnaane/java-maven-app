pipeline {
  agent any
  stages {
    stage("build") {
      stage {
        exho ("building the app")
      }
    }
    tage("test") {
      stage {
        exho ("testing the app")
      }
    }
    tage("deploy") {
      stage {
        exho ("deploying the app")
      }
    }
    tage("publish") {
      stage {
        exho ("publishing the app")
      }
    }
  }
}
